<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="4dp"
                android:color="#fff" />
        </shape>
    </item>
    <item>
        <shape>
            <!--圆角-->
            <corners android:radius="10dp"/>
            <!--描边-->
            <stroke android:width="4dp"
                android:color="#fff"
                android:dashWidth="5dp"
                android:dashGap="0dp" />
        </shape>
    </item>
</layer-list>